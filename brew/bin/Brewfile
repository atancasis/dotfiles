# Install GNU core utilities (those that come with macOS are outdated)
brew 'coreutils'

# Install GNU `find`, `locate`, `updatedb`, and `xargs`, g-prefixed
brew 'findutils'

# Install Binaries
brew 'git'
brew 'tree'
brew 'mackup'
brew 'wget'
brew 'stow'
brew 'neovim'
brew 'ripgrep'
brew 'tmux'
brew 'fzf'
